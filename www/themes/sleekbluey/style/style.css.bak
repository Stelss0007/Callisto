html,body{
  padding:0px;
  margin:0px;
  font-family:Arial, Helvetica, sans-serif;
  font-size:11px;
  background:#86addc url(/themes/sleekbluey/images/bg.gif) repeat-x top center;
  width:100%;
}

div#page{
  width:100%;
  min-width:870px;
  text-indent: 0px;
  margin:0px;
}


/* Шапка */
#header_title{
	color:#ffffff;
	font-size:31px;
	font-family: Verdana, Arial, Helvetica, sans-serif;
	padding-top:30px;
	padding-left:25px;
  margin:0px;
}

#header_title span{
  color:#ffffff;
  font-size:11px;
  font-family: Verdana, Arial, Helvetica, sans-serif;
  position:relative;
  top:-5px;
}

div.top_menu_items {
  float:left;
  margin:0px;
  margin-left:15px;
  padding-top:4px;
  background:url(/themes/sleekbluey/images/menu.gif) repeat-x center left;
  height:22px;
  width:80px;
  position:relative;
  text-indent: 0px;
}

div.top_menu_items a{
  color:#FFFFFF;
  font-family:Verdana, Arial, Helvetica, sans-serif;
  font-size:14px;
  text-decoration: none;
  text-indent: 0px;
  margin:0px;
}

div.top_menu_items a:hover{
  color:#FFFFFF;
  text-decoration:underline;
  text-indent: 0px;
  margin:0px;
}

#menu_search_box{
  font-family:Verdana, Arial, Helvetica, sans-serif;
  color:#7f7f7f;
  font-size:12px;
  float:left;
  width:100%;
  padding-top:6px;
  padding-left:8px;
  text-indent: 0px;
  margin-top:0px;
}

#menu_search_box input.search_string{
  width:250px;
  font-size:14px;
  color:#666666;
  border:0px;
  background:none;
  margin-top:0px;
}
/* End Шапка */

/* Блоки */
div.block{
  width:188px;
  padding-left:12px;
  padding-top:2px;
  margin:0px;
  margin-left:0px;
  float:left;
  background:url(/themes/sleekbluey/images/body_top_bg.gif) no-repeat right top;
  text-indent: 0px;
  font-size:12px;
  font-family:Arial, Helvetica, sans-serif;
}

.block ul{
  padding:0px;
  margin:0px;
}

.block li{
  padding:0px;
  margin:0px;
  list-style-type:none;
}

.block h2{
  color:#003471;
  font-size:18px;
  font-family:Arial, Helvetica, sans-serif;
  margin:0px;
  padding-bottom:5px;
  padding-top:15px;
  padding-left:8px;
  font-weight:normal;
  background:url(/themes/sleekbluey/images/h2_li.gif) no-repeat left center;
  height:31px;
}

.block ul li.widget_categories ul li a{
  background: url(/themes/sleekbluey/images/cat_li.gif) left top no-repeat;
  padding-left:9px;
}
.block ul li.widget_categories ul li a:hover{
  text-decoration:underline;
}

.block li a{
  color:#2b2e31;
  font-size:12px;
  font-family:Verdana, Arial, Helvetica, sans-serif;
  text-decoration:none;
  font-weight:normal;
  line-height:19px;
}

.block ul li ul li{
  border-bottom:0;
  padding-left:9px;
  padding-right:9px;
  background: url(/themes/sleekbluey/images/solid_li.gif) center bottom no-repeat;
}

/* end блоки */




/* Тело страницы */
